    <script>
        function closes() {
            $("#pageLoading").fadeOut("fast", function () { $(this).remove(); });
        }
        var pc;
        $.parser.onComplete = function () {
            if (pc) clearTimeout(pc);
            pc = setTimeout(closes, 1000);
            closes();
        }
        function ShowLoading(text) {
            // $(".window-mask").show();
            $("#loading").html(text);
            $("#loading").show();
            wrapper.ShowMask();
        }

        function HideLoading() {
            // $(".window-mask").show();
            
            $("#loading").hide();
            wrapper.HideMask();
        }
    </script>


    <link href="~/Content/css/plugin_extend.css" rel="stylesheet" />
    <style>
        #toolbar1, #toolbar2 {
            border-bottom: none;
        }

        .table_content {
            width: 100%;
            background: #fff;
            border: none;
        }

            .table_content td {
                border-bottom: 1px solid #ccc;
                padding: 5px;
            }

                .table_content td.td_title {
                    width: 90px;
                    text-align: center;
                    font-size: 12px;
                    font-weight: bold;
                    background: #fafafa;
                    border-right: 1px solid #ccc;
                }

        .textbox, .textbox-text {
            border: none;
            border-radius: 0px;
            box-shadow: none;
            margin: 0px;
            padding: 0px;
        }

        .textbox-fucused {
            box-shadow: none;
        }
        .layout_north_right{
            width: 500px;
        }
        .language {
          float:right;
        }
        .login_info {
            float: right;
        }
    </style>

</head>


<body id="homebody" class="easyui-layout"> 

    <div id="loading" style="z-index: 1000; width: auto; display:none; height: 16px; line-height: 16px; position: absolute; top: 50%; left: 50%; margin-top: -20px; margin-left: -92px; border: 2px dashed #75186D; padding: 12px 5px 10px 30px; background: #ffffff url('/Scripts/03jeasyui/jeasyui_extend/images/ajax-loader.gif') no-repeat scroll 5px center;">
        

        @MyUIHelper.Get((System.Data.DataTable)ViewData["dictionary"],"loading")

    </div>
    <div id='pageLoading' style="position: absolute; z-index: 1000; top: 0px; left: 0px; width: 100%; height: 100%; background: #545669; text-align: center; opacity: 0.8; filter: alpha(opacity=80);">
        <div style="z-index: 3; width: auto; height: 16px; line-height: 16px; position: absolute; top: 50%; left: 50%; margin-top: -20px; margin-left: -92px; border: 2px dashed #75186D; padding: 12px 5px 10px 30px; background: #ffffff url('/Scripts/03jeasyui/jeasyui_extend/images/ajax-loader.gif') no-repeat scroll 5px center;">
            @MyUIHelper.Get((System.Data.DataTable)ViewData["dictionary"], "loading")
        </div>
    </div>
    <div id="dialog1"><div id="dialog"></div></div>
    <noscript>
