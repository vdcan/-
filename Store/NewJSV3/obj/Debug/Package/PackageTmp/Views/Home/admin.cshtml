    <script type="text/javascript">
        function Cleandialog() {
            //   alert($("#dialog").html());
            //  alert($('#dialog1').html())
            console.log("Cleandialog");

            var list = document.getElementById("dialog1");
            list.removeChild(list.childNodes[0]);

            $('#dialog1').html("");
            $('#dialog1').html("<div id='dialog'></div>");

        }
        wrapper.model = null;
        var model_string = '@Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(Model))';


