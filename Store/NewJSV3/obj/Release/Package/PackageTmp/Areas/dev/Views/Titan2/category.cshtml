}

<div class="easyui-layout" data-options="fit:true" style=" overflow:hidden;">
    <div id="north_panel" data-options="region:'north',border:false" style="background-color: #f3f3f3;">
        <div class="km_toolbar">
            <div id="km_toolbar" style="float:left;"></div><div id="km_toolbar2" style="float:left;"></div>
        </div>
        <div id="km_toolbar_2" class="km_toolbar" style="display:none">
            <a id="toolbar_delete" href="javascript:void(0)" class="easyui-linkbutton" data-options="plain:true,iconCls:'icon-undo'" title="@MyUIHelper.Get((System.Data.DataTable)ViewData["dictionary"], "button_name")" onclick="km.toolbar.do_undo();">@MyUIHelper.Get((System.Data.DataTable)ViewData["dictionary"], "button_name")</a>
            <a id="toolbar_save" href="javascript:void(0)" class="easyui-linkbutton" data-options="plain:true,iconCls:'icon-save'" title="@MyUIHelper.Get((System.Data.DataTable)ViewData["dictionary"], "button_name")" onclick="km.toolbar.do_save();">@MyUIHelper.Get((System.Data.DataTable)ViewData["dictionary"], "button_name")</a>
        </div>
    </div>
    <div id="main-panel_a" data-options="region: 'west',border:true" style='width: 50%;'>

        <table id="category"></table>

    </div>
    <div id="main-panel_b" data-options="region: 'center',border:true" style='width: 50%;'>




        <form id="category_content" class="form_content" method="post">
            <table class="table_content" cellpadding="3" cellspacing="0" border="0">

                <tr>
                    <td class="td_title">@MyUIHelper.Get((System.Data.DataTable)ViewData["dictionary"], "enabled") </td>
                    <td>
                        <input id='TPL_active_flag0' name='active_flag' type='radio' checked value='1' />@MyUIHelper.Get((System.Data.DataTable)ViewData["dictionary"], "yes")

                        <input id='TPL_active_flag1' name='active_flag' type='radio' value='0' />@MyUIHelper.Get((System.Data.DataTable)ViewData["dictionary"], "no")
                    </td>
                </tr>

                <tr><td class="td_title">@MyUIHelper.Get((System.Data.DataTable)ViewData["dictionary"], "add_by")</td> <td>  <input id='TPL_add_by' data-options="" name='add_by' type='text' class='easyui-textbox' style='width:100px' /></td></tr>

                <tr><td class="td_title">@MyUIHelper.Get((System.Data.DataTable)ViewData["dictionary"], "add_on")</td> <td>  <input id='TPL_add_date' data-options="" name='add_date' type='text' class='easyui-textbox' style='width:230px' /></td></tr>

                <tr><td class="td_title">@MyUIHelper.Get((System.Data.DataTable)ViewData["dictionary"], "description")</td> <td>  <input id='TPL_category_description' data-options="" name='category_description' type='text' class='easyui-textbox' style='width:500px' /></td></tr>

                <tr><td class="td_title">@MyUIHelper.Get((System.Data.DataTable)ViewData["dictionary"], "class name")</td> <td>  <input id='TPL_category_name' data-options="" name='category_name' type='text' class='easyui-textbox' style='width:500px' /></td></tr>

                <tr>
                    <td class="td_title">@MyUIHelper.Get((System.Data.DataTable)ViewData["dictionary"], "delete flag") </td>
                    <td>
                        <input id='TPL_delete_flag0' name='delete_flag' type='radio' checked value='1' />@MyUIHelper.Get((System.Data.DataTable)ViewData["dictionary"], "yes")

                        <input id='TPL_delete_flag1' name='delete_flag' type='radio' value='0' />@MyUIHelper.Get((System.Data.DataTable)ViewData["dictionary"], "no")
                    </td>
                </tr>
                <input id='TPL_id' name='id' type='hidden' value='0' />

                <tr><td class="td_title">@MyUIHelper.Get((System.Data.DataTable)ViewData["dictionary"], "sort")</td> <td>  <input id='TPL_sort_number' data-options="" name='sort_number' type='text' class='easyui-textbox' style='width:100px' /></td></tr>
            </table>
        </form>


    </div>
</div>
<div style='display:none'>

</div>
